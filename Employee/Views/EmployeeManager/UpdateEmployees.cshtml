
@{
	ViewBag.Title = "Index";
}

<div ng-app="APIModule">
	<div ng-controller="APIController">
		<div class="row">
			<div class="col-md-12">
				<div class="pull-left">
					<h2>Employee</h2>
				</div>
			</div>
		</div>

		<div class="form-group">
			<input type="number" class="form-control" id="id" placeholder="id" ng-model="empId" />
			<input type="text" class="form-control" id="FirstName" placeholder="FirstName" ng-model="FirstName" />
			<input type="text" class="form-control" id="LastName" placeholder="LastName" ng-model="LastName" />
			<input type="text" class="form-control" id="Active" placeholder="Active" ng-model="Active" />
			<input type="text" class="form-control" id="UserName" placeholder="UserName" ng-model="UserName" />
			<input type="text" class="form-control" id="Password" placeholder="Password" ng-model="Password" />
			<input type="number" class="form-control" id="Mobile" placeholder="Mobile" maxlength="10" ng-model="Mobile" />
			<input type="text" class="form-control" id="Address1" placeholder="Address1" ng-model="Address1" />
			<input type="text" class="form-control" id="Address2" placeholder="Address2" ng-model="Address2" />
			<input type="text" class="form-control" id="City" placeholder="City" ng-model="City" />
			<input type="text" class="form-control" id="State" placeholder="State" ng-model="State" />
			<input type="text" class="form-control" id="Zip" placeholder="Zip" ng-model="Zip" />
			<input type="date" class="form-control" id="Dob" placeholder="Dob" ng-model="Dob" />
			<input type="date" class="form-control" id="JoiningDate" placeholder="JoiningDate" ng-model="JoiningDate" />


			@*<input type="email" class="form-control" id="email" placeholder="Enter email" [required="string" ] data-ng-model="mailid" />*@
		</div>
		<button type="button" class="btn btn-default" ng-click=updateEmployees()>Udate</button>
		<button type="button" class="btn btn-default" ng-click=clicked()>Back</button>

	</div>
</div>

