
@{
	ViewBag.Title = "Index";
}
<div ng-controller="APIController">
	<div ng-init="populateRecords()">
		<div class="row">
			<div class="col-md-12">
				<div class="pull-left">
					<h2>Update Manager</h2>
				</div>
			</div>
		</div>

		<div class="form-group">
			<table>


				<tr>
					<td><label>FirstName</label></td>
					<td><input type="text" class="form-control" id="FirstName" placeholder="FirstName" ng-model="employee.FirstName" required /></td>
				</tr>


				<tr>

					<td><label>LastName</label></td>
					<td><input type="text" class="form-control" id="LastName" placeholder="LastName" ng-model="employee.LastName" /></td>

				</tr>

				@*<tr>
						<td><label>Active</label></td>
						<td><input type="text" class="form-control" id="Active" placeholder="Active" ng-model="employee.Active" /></td>
					</tr>*@
				<tr>
					<td><label>UserName</label></td>
					<td><input type="email" class="form-control" id="UserName" placeholder="UserName" ng-model="employee.UserName" required /></td>
				</tr>
				@*<tr>

						<td><label>Password</label></td>
						<td><input type="password" class="form-control" id="Password" placeholder="Password" ng-model="employee.Password" /></td>
					</tr>*@
				@*<tr>

						<td><label>Re-Password</label></td>
						<td><input type="password" class="form-control" id="RePassword" placeholder="Re-Password" ng-model="employee.RePassword" /></td>
					</tr>*@
				<tr>

					<td><label>Mobile</label></td>
					<td><input type="number" class="form-control" id="Mobile" placeholder="Mobile" maxlength="10" ng-model="employee.Mobile" max="10" /></td>
				</tr>

				<tr>

					<td><label>Address1</label></td>
					<td><input type="text" class="form-control" id="Address1" placeholder="Address1" ng-model="employee.Address1" /></td>
				</tr>
				<tr>

					<td><label>Address2</label></td>
					<td><input type="text" class="form-control" id="Address2" placeholder="Address2" ng-model="employee.Address2" /></td>
				</tr>
				<tr>

					<td><label>City</label></td>
					<td><input type="text" class="form-control" id="City" placeholder="City" ng-model="employee.City" /></td>
				</tr>
				<tr>

					<td><label>State</label></td>
					<td><input type="text" class="form-control" id="State" placeholder="State" ng-model="employee.State" /></td>
				</tr>
				<tr>

					<td><label>Zip</label></td>
					<td><input type="number" class="form-control" id="Zip" placeholder="Zip" ng-model="employee.Zip" /></td>
				</tr>
				<tr>

					<td><label>Dob</label></td>
					<td><input type="date" class="form-control" id="Dob" placeholder="Dob" ng-model="employee.Dob" /></td>
				</tr>
				<tr>
					<td><label>JoiningDate</label></td>

					<td><input type="date" class="form-control" id="JoiningDate" placeholder="JoiningDate" ng-model="employee.JoiningDate" /></td>
				</tr>
				<tr>
					<td><label>Role</label></td>

					<td><input type="text" class="form-control" id="Role" placeholder="Role" ng-model="employee.Role" /></td>
				</tr>

				@*<input type="email" class="form-control" id="email" placeholder="Enter email" [required="string" ] data-ng-model="mailid" />*@

				<tr>
					@*<td>
						<button type="button" class="btn btn-default" ng-click="saveManagers()">Insert</button>
					</td>*@
					<td>
						<button type="button" class="btn btn-default" ng-click="updateManagers()">Update</button>
					</td>
					<td>
						<button type="button" class="btn btn-default" ng-click="back()">Back</button>
					</td>
				</tr>
			</table>
		</div>
	</div>
</div>
